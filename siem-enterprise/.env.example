# .env.example

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_USER=your_username
DB_PASSWORD=your_password
DB_NAME=your_database_name

# Security Settings
SECRET_KEY=your_secret_key_here
JWT_SECRET=your_jwt_secret_here

# Server Configuration
HOST=0.0.0.0
PORT=8080

# Third-Party Services
# API Keys
SERVICE_ONE_API_KEY=your_service_one_api_key
SERVICE_TWO_API_KEY=your_service_two_api_key

# Other Configuration
DEBUG=False
LOG_LEVEL=info

# Production Defaults
# Change accordingly
ENV=production
